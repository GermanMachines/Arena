            <TableColumn fx:id="colNomEquipe" minWidth="0.0" prefWidth="110.0" resizable="false" text="NomEquipe" />
            <TableColumn fx:id="colRegionEq" minWidth="0.0" prefWidth="90.0" resizable="false" text="Region" />
            <TableColumn fx:id="colScoreEq" prefWidth="113.0" resizable="false" text="Score" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <styleClass>
            <String fx:value="table-view" />
