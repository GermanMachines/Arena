<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>

<AnchorPane id="AnchorPane" prefHeight="272.0" prefWidth="557.0" stylesheets="@GamesStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.arena.gui.ItemPostController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="23.0" layoutY="8.0" prefHeight="244.0" prefWidth="557.0">
         <children>
            <Label fx:id="titreLabel" prefHeight="20.0" prefWidth="184.0" text="titre">
               <font>
                  <Font name="Cambria Bold" size="15.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="160.0">
               <children>
                  <Label fx:id="auteurlabel" prefHeight="20.0" prefWidth="312.0" text="auteur">
                     <font>
                        <Font name="Cambria Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="datapostlabel" prefHeight="21.0" prefWidth="311.0" text="date">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Label>
               </children>
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <ImageView fx:id="img" fitHeight="125.0" fitWidth="294.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="conteur" />
            <Button mnemonicParsing="false" onAction="#like" styleClass="search-button" stylesheets="@GamesStyle.css" text="like" textFill="WHITE" />
            <Rating fx:id="PostRarting" rating="1.0" style="-fx-background-color: red;" stylesheets="@GamesStyle.css" />
            <Button mnemonicParsing="false" onAction="#comentpostbtn" prefHeight="28.0" prefWidth="107.0" styleClass="search-button" stylesheets="@GamesStyle.css" text="commentaire" textFill="WHITE">
               <font>
                  <Font name="Candara Bold" size="15.0" />
               </font>
            </Button>
            <Label fx:id="tfidpost" text="id" />
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="item-game" />
      <String fx:value="shadow" />
   </styleClass>
</AnchorPane>
