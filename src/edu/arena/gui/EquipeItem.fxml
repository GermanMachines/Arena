<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@GamesStyle.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="edu.arena.gui.EquipeItemController">
   <children>
      <VBox layoutX="218.0" layoutY="139.0" prefHeight="329.0" prefWidth="326.0" styleClass="available-games-card" stylesheets="@GamesStyle.css">
         <children>
            <HBox alignment="CENTER" prefHeight="51.0" prefWidth="225.0">
               <children>
                  <Label fx:id="nameLabel" prefHeight="41.0" prefWidth="218.0" text="nom equipe">
                     <font>
                        <Font name="Cambria Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="tfid" text="Label" />
                  <Label fx:id="imagenom" text="Label" />
               </children>
            </HBox>
            <Label fx:id="score" text="Score" />
            <ImageView fx:id="image" fitHeight="150.0" fitWidth="325.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="Region" text="Label" />
            <Button fx:id="add" mnemonicParsing="false" onAction="#AddUser" prefHeight="56.0" prefWidth="117.0" styleClass="add-btn" stylesheets="@GamesStyle.css" text="Appartenir" textFill="#d03434">
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial Black" size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
